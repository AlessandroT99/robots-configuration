<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE robot PUBLIC "-//YARP//DTD yarprobotinterface 1.0//EN" "http://www.yarp.it/DTD/yarprobotinterfaceV1.0.dtd">

<robot name="iCubGenovaV3" build="1" portprefix="icub">
    
    <params file="hardware/electronics/pc104.xml" />

    
    <!-- HEAD - MC -->
    <devices file="./hardware/motorControl/head-eb20-j0_1-mc.xml" />  
    <devices file="./hardware/motorControl/head-eb21-j2_5-mc.xml" /> 
    <devices file="./wrappers/motorControl/head-mc_wrapper.xml" /> 
 
    <!-- FACE - MC-->
    <devices file="./hardware/motorControl/face-eb22-j0_1-mc.xml" />  
    <devices file="./hardware/motorControl/face-eb23-j2_5-mc.xml" /> 
    <devices file="./wrappers/motorControl/face-mc_wrapper.xml" />  

    <!-- LEFT UPPER ARM - MC-->
    <devices file="hardware/motorControl/left_arm-eb1-j0_1-mc.xml" />
    <devices file="hardware/motorControl/left_arm-eb2-j2_3-mc.xml" />
    <devices file="wrappers/motorControl/left_upperarm-mc_wrapper.xml" /> 

    <!-- LEFT FOREARM - MC-->
    <devices file="hardware/motorControl/left_arm-eb24-j4_7-mc.xml" />
    <devices file="hardware/motorControl/left_arm-eb25-j8_11-mc.xml" />
    <devices file="hardware/motorControl/left_arm-eb26-j12_15-mc.xml" />
    <devices file="wrappers/motorControl/left_forearm-mc_wrapper.xml" /> 


    <!-- RIGHT FOREARM - MC -->
    <devices file="hardware/motorControl/right_arm-eb27-j4_7-mc.xml" />
    <devices file="hardware/motorControl/right_arm-eb28-j8_11-mc.xml" />
    <devices file="hardware/motorControl/right_arm-eb29-j12_15-mc.xml" />
    <devices file="wrappers/motorControl/right_forearm-mc_wrapper.xml" /> 

    
    <!-- CALIBRATORS -->
    <devices file="calibrators/head-calib.xml" />
    <devices file="calibrators/face-calib.xml" /> 
    <devices file="calibrators/left_upperarm-calib.xml" />
    <devices file="calibrators/left_forearm-calib.xml" />
    <devices file="calibrators/right_forearm-calib.xml" />

   
</robot> 
