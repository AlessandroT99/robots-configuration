<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE devices PUBLIC "-//YARP//DTD yarprobotinterface 1.0//EN" "http://www.yarp.it/DTD/yarprobotinterfaceV1.0.dtd">

<devices robot="iCubGenova01" build="1">
	<device name="right_arm_accelerometers_mtb_wrapper" type="analogServer">
		<param name="period">       10  				</param>
		<param name="deviceId">     right_arm_accelerometers			</param>
		
		<action phase="startup" level="6" type="attach">
		    <paramlist name="networks">
		        <elem name="FirstDevice">  right_arm_accelerometers_mtb </elem>
		    </paramlist>
		</action>

		<action phase="shutdown" level="6" type="detach" />
	</device>
</devices>
