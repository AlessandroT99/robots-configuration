<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE params PUBLIC "-//YARP//DTD robotInterface 1.0//EN" "http://www.icub.org/DTD/robotInterfaceV1.0.dtd">

<params robot="iCubGenova02" build="1">

    <params file="hardware/electronics/pc104.xml" />
    
    <group name="ETH_BOARD">
   
        <group name="ETH_BOARD_PROPERTIES">
            <param name="IpAddress">                10.0.1.X             </param> <!-- Usually it is the number of board on schematics, but it can be changed -->
            <param name="IpPort">                   12345                </param> <!-- number op IP port used for communication between firmware and yarprobotinterface -->
            <param name="Type">                     EMS4                 </param>
            <param name="maxSizeRXpacket">          768                  </param>
            <param name="maxSizeROP">               384                  </param>
            <param name="maxNumberRegularROPs">     32                   </param>    
            <param name="protocolCapability">       MAXIMUM              </param>   
        </group>

        <group name="ETH_BOARD_SETTINGS">
            <param name="Name">                     "body_part-ebX-jA_B"  </param> <!-- this string is used to print info/debug/warning/error by the logger -->
            <param name="regularsTXrate">           5                    </param> <!-- this is the period of sending info by firmware --> 
            <param name="protocolToUse">            STANDARD             </param>  
        </group>

    </group>  
    
</params>

