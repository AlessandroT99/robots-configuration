<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE robot PUBLIC "-//YARP//DTD robotInterface 1.0//EN" "http://www.icub.org/DTD/robotInterfaceV1.0.dtd">

    <robot name="iCubErzelli02" portprefix="icub">
    
    <params file="hardware/electronics/pc104.xml" />
    
    <!-- HEAD -->
    <devices file="./hardware/motorControl/head-eb20-j0_1-mc.xml" />  
    <devices file="./hardware/motorControl/head-eb21-j2_5-mc.xml" /> 
    <devices file="./wrappers/motorControl/head-mc_wrapper.xml" /> 
 
  

    <!-- LEFT ARM -->
    <devices file="hardware/motorControl/left_arm-eb1-j0_3-mc.xml" />
    <devices file="hardware/motorControl/left_arm-eb24-j4_7-mc.xml" />
    <devices file="hardware/motorControl/left_arm-eb25-j8_11-mc.xml" />
    <devices file="hardware/motorControl/left_arm-eb26-j12_15-mc.xml" />
    <devices file="wrappers/motorControl/left_arm-mc_wrapper.xml" /> 

    <!-- RIGHT ARM -->
    <devices file="hardware/motorControl/right_arm-eb3-j0_3-mc.xml" />
    <devices file="hardware/motorControl/right_arm-eb27-j4_7-mc.xml" />
    <devices file="hardware/motorControl/right_arm-eb28-j8_11-mc.xml" />
    <devices file="hardware/motorControl/right_arm-eb29-j12_15-mc.xml" />
    <devices file="wrappers/motorControl/right_arm-mc_wrapper.xml" />

 
    <!-- INERTIAL SENSOR--> 
    <devices file="hardware/inertials/head-inertial.xml" /> 
    
    <!-- ANALOG SENSOR MAIS -->
    <devices file="wrappers/MAIS/left_arm-mais_wrapper.xml" /> 
    <devices file="wrappers/MAIS/right_arm-mais_wrapper.xml" /> 
    <devices file="hardware/MAIS/left_arm-eb26-j12_15-mais.xml" /> 
    <devices file="hardware/MAIS/right_arm-eb29-j12_15-mais.xml" />  
    
   
    
    <!-- ANALOG SENSOR FT -->
    <devices file="wrappers/FT/left_arm-FT_wrapper.xml" /> 
    <devices file="wrappers/FT/right_arm-FT_wrapper.xml" />
    
    <devices file="hardware/FT/left_arm-eb1-j0_3-strain.xml" /> 
    <devices file="hardware/FT/right_arm-eb3-j0_3-strain.xml" />
   

    <!-- VIRTUAL ANALOG SENSORS (WRAPPER ONLY) -->
    <devices file="wrappers/VFT/left_arm-VFT_wrapper.xml" /> 
    <devices file="wrappers/VFT/right_arm-VFT_wrapper.xml" /> 
    
    <!--  CALIBRATORS -->
    <devices file="calibrators/left_arm-calib.xml" />
    <devices file="calibrators/right_arm-calib.xml" /> 
    <devices file="calibrators/head-calib.xml" />
   
   
    
    </robot> 
